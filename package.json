        "deploy:local": "./node_modules/.bin/hardhat --config hardhat.config.local.js --network localhost deploy-collection",
        "deploy:lottery": "./node_modules/.bin/hardhat --config hardhat.config.local.js --network localhost deploy-lottery",
        "test:lottery": "./node_modules/.bin/hardhat --config hardhat.config.local.js --network localhost test-lottery",
